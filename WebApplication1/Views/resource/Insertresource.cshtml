    <script type="text/javascript">
        var vm = new Vue({
            el: "#div123",
            data: {
                nums: "",
                ead: "",
                three: "",
                k: "",
                shitu: "",
                whei:"",
                xcbzq:"",
            },
            
            methods: {
                load: function () {
                    var t = this;
                    $.ajax({
                        url: "/resource/Selectone",
                        type:"post",
                        success: function (msg) {
                            var dt = eval("(" + msg + ")");
                            t.nums = dt;
                        }
                    })
                },
                xcbz: function () {
                    var t = this;
                    $.ajax({
                        url: "/resource/Insertresource",
                        type:"post",
                        success: function (msg) {
                            var dt = eval("(" + msg + ")");
                            t.xcbzq = dt;
                        }
                    })
                },
                bian: function () {
                    var t = this;
                    var id = $("#firstKind").val();
                    $.ajax({
                        url: "/resource/Selecttwo/" + id,
                        type: "post",
                        success: function (msg) {
                            var dt = eval("(" + msg + ")");
                            t.ead = dt;
                        }
                    })
                },
                gai: function () {
                    var t = this;
