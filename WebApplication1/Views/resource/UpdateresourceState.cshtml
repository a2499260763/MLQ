@model resourceModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UpdateresourceState</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <link href="~/css/cwcalendar.css" rel="stylesheet" />
    <script src="~/javascript/comm/comm.js"></script>
    <script src="~/javascript/comm/list.js"></script>
    <script src="~/javascript/calendar-ch.js"></script>
    <script src="~/javascript/jquery-1.7.2.js"></script>
    <script src="~/javascript/locate.js"></script>
    <script src="~/javascript/select.js"></script>
    <script src="~/javascript/vue-2.4.0.js"></script>
    <script type="text/javascript">
        window.onload = check;
        function tick() {
            
            var now = new Date();
            var hours, minutes, seconds, noon;
            var intHours, intMinutes, intSeconds;
            intHours = now.getHours();
            intMinutes = now.getMinutes();
            intSeconds = now.getSeconds();
            if (intHours < 24) {
                hours = intHours + ":";
                noon = "A.M.";
            } else {
                intHours = intHours - 24;
                hours = intHours + ":";
                noon = "P.M.";
            }
            if (intMinutes < 10) {
                minutes = "0" + intMinutes + ":";
            } else {
                minutes = intMinutes + ":";
            }
            if (intSeconds < 10) {
                seconds = "0" + intSeconds + " ";
            } else {
                seconds = intSeconds + " ";
            }
            timeString = hours + minutes + seconds;
            var now = new Date();
            document.getElementById("nowTime").value = now.getFullYear() + "-" + (now.getMonth() + 1) + "-" + now.getDate() + " " + timeString;
            window.setTimeout("tick();", 1000);
        }

        //load事件
        function check() {
            //获得系统当前时间的方法
            tick();
        }
        $(function () {
            
            
            if ("@Model.RSex" == "男") {
                $("#RSex").val("男");
            } else {
                $("#RSex").val("女");
            }

            if ("@Model.RNationality" == "中国") {
                $("#RNationality").val("中国");
            } else {
                $("#RNationality").val("美国");
            }

            if ("@Model.RNation" == "汉族") {
                $("#RNation").val("汉族");
            } else {
                $("#RNation").val("回族");
            }
            if ("@Model.RFaith" == "无") {
                $("#RFaith").val("无");
            } else {
                $("#RFaith").val("佛教");
            }
            if ("@Model.RPolitics" == "群众") {
                $("#RPolitics").val("群众");
            } else {
                $("#RPolitics").val("党员");
            }
            if ("@Model.RBackground" == "高中") {
                $("#RBackground").val("高中");
            } else if ("@Model.RBackground" == "本科") {
                $("#RBackground").val("本科");
            } else {
                $("#RBackground").val("大专");
            }

            if ("@Model.RYears" == "12") {
                $("#RYears").val("12");
            } else {
                $("#RYears").val("16");
            }
            if ("@Model.REducation" == "生物工程") {
                $("#REducation").val("生物工程");
            } else {
                $("#REducation").val("计算机");
            }
            if ("@Model.RSalary" == "薪酬标准1") {
                $("#RSalary").val("薪酬标准1");
            } else {
                $("#RSalary").val("薪酬标准2");
            }
            if ("@Model.Rstrong" == "java") {
                $("#Rstrong").val("java");
            } else {
                $("#Rstrong").val("数据库2");
            }
            if ("@Model.RHobby" == "篮球") {
                $("#RHobby").val("篮球");
            } else {
                $("#RHobby").val("跳舞");
            }
        })

    </script>

</head>
