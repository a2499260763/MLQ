    <script type="text/javascript">
        var vm = new Vue({
            el: "#div123",
            data: {
                xcbzq:"",
            },

            methods: {
                
                xcbz: function () {
                    var t = this;
                    $.ajax({
                        url: "/resource/Selectxcbz",
                        type:"post",
                        success: function (msg) {
                            var dt = eval("(" + msg + ")");
                            t.xcbzq = dt;
                        }
                    })
                },
            },
            created: function () {
                this.xcbz();
            }
        })
