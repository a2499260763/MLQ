@model MianShiModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MianShiRegister</title>
    <link rel="stylesheet"
          href="../../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../../css/cwcalendar.css"
          type="text/css">
    <script type="text/javascript"
            src="../../../javascript/comm/comm.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/list.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/calendar-ch.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/jquery-1.7.2.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/locate.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/select.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/time.js">
    </script>
    <script type="text/javascript">
 	function search(){
 	if(document.getElementById("date").value==""){
 	alert("请选择面试时间");
 	}else{
 	document.interviewForm.submit();
 	}}
    </script>

</head>

<body>
    @using (Html.BeginForm("MianShiRegisterUpdate", "MianShi", FormMethod.Post))
    {
        <input type="hidden" name="Id" value="@Model.Id" />
        <input type="hidden" name="MSShaiXueRen" value="@Model.Id" />
        <input type="hidden" name="MSShaiXueTime" value="2019-10-11" />
        <input type="hidden" name="JlZcOverTime" value="@Model.JlZcOverTime" />
        <input type="hidden" name="MSShengQingYiJian" value="@Model.Id" />
        <table width="100%">
            <tr>
                <td>

                    <font color="black">您正在做的业务是：人力资源--招聘管理--面试管理--面试结果登记  </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="面试登记" class="BUTTON_STYLE1">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">

                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="10%">
                    职位分类
                </td>
                <td width="10%" class="TD_STYLE2">
                    <input type="text" name="MKName" value="@Model.MKName" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td width="10%" class="TD_STYLE1">
                    职位名称
                </td>
                <td width="15%" class="TD_STYLE2">
                    <input type="text" name="MCName" value="@Model.MCName" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td width="10%" class="TD_STYLE1">
                    招聘类型
                </td>
                <td width="20%" class="TD_STYLE2" colspan="2">
                    <input type="text" name="ZPName" value="@Model.ZPName" readonly="readonly" class="INPUT_STYLE2">
                </td>

                <td width="10%" rowspan="6" align="center"></td>

            </tr>

            <tr>
                <td class="TD_STYLE1">
                    姓名
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlName" value="@Model.JlName" readonly="readonly" class="INPUT_STYLE2">

                </td>
                <td class="TD_STYLE1">
                    性别
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlSex" value="@Model.JlSex" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    EMAIL
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="JlEmail" value="@Model.JlEmail" readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    电话号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlDianHua" value="@Model.JlDianHua" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    家庭电话
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlJiaTingDianHua" value="@Model.JlJiaTingDianHua" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    手机
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="JlShouJi" value="@Model.JlShouJi" readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    住址
                </td>
                <td colspan="3" class="TD_STYLE2">
                    <input type="text" name="JlAdds" value="@Model.JlAdds" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    邮编
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="JlYouBian" value="@Model.JlYouBian" readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    国籍
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlGuoJi" value="@Model.JlGuoJi" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    出生地
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlBreathAdds" value="@Model.JlBreathAdds" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    生日
                </td>
                <td class="TD_STYLE2" colspan="2">
                    <input type="text" name="JlBreathDay" value="@Model.JlBreathDay" readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>

            <tr>
                <td width="11%" class="TD_STYLE1">
                    民族
                </td>
                <td class="TD_STYLE2" width="14%">
                    <input type="text" name="JlMingZu" value="@Model.JlMingZu" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    宗教信仰
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlZongJioaXingYang" value="@Model.JlZongJioaXingYang" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    政治面貌
                </td>
                <td class="TD_STYLE2" colspan="2">
                    <input type="text" name="JlZhengZhiMianMao" value="@Model.JlZhengZhiMianMao" readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>

                <td class="TD_STYLE1">
                    身份证号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlEntityCond" value="@Model.JlEntityCond" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    年龄
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlAge" value="@Model.JlAge" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    毕业学校
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlBiYanShooln" value="@Model.JlBiYanShooln" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1" width="10%">
                    学历
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlXueli" value="@Model.JlXueli" readonly="readonly" class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    教育年限
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlJiaoYvYean" value="@Model.JlJiaoYvYean" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    学历专业
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlXueLiZhuanYe" value="@Model.JlXueLiZhuanYe" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    薪酬要求
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlXingZiYaoQiu" value="@Model.JlXingZiYaoQiu" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    注册时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlZcTime" value="@Model.JlZcTime" readonly="readonly" class="INPUT_STYLE2">

                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    特长
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlTeChang" value="@Model.JlTeChang" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    爱好
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlAiHoa" value="@Model.JlAiHoa" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    <span style="font-size: 10.5pt; font-family: 宋体;"></span>
                    推荐人
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlTuiJianRen" value="@Model.JlTuiJianRen" readonly="readonly" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    <span style="font-size: 10.5pt; font-family: 宋体;"></span>
                    推荐时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="JlTuiJianTime" value="@Model.JlTuiJianTime" readonly="readonly" class="INPUT_STYLE2">

                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    个人履历
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="JlGeRenLvLi" rows="4"
                              class="TEXTAREA_STYLE1" readonly="readonly">@Model.JlGeRenLvLi</textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    备注
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="JlBeiZhu" rows="4" class="TEXTAREA_STYLE1" readonly="readonly">@Model.JlBeiZhu</textarea>
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    推荐意见
                </td>
                <td class="TD_STYLE2" colspan="7">
                    <textarea name="@Model.JlYesNo" rows="4" class="TEXTAREA_STYLE1" readonly="readonly">@ViewBag.yes</textarea>
                </td>
            </tr>


            <tr>
                <td class="TD_STYLE1">
                    面试次数
                </td>
                <td class="TD_STYLE2">
                    <input type="hidden" value="@Model.MSCiShu" name="MSCiShu" />
                    <input type="text" value="第 @Model.MSCiShu 次面试" class="INPUT_STYLE2" readonly="readonly" />
                </td>
                <td class="TD_STYLE1">
                    形象评价
                </td>
                <td class="TD_STYLE2">
                    <select name="MSXingXiangPingJia" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    口才评价
                </td>
                <td class="TD_STYLE2">
                    <select name="MSKouCaiPingJia" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    外语口语能力
                </td>
                <td class="TD_STYLE1">
                    <select name="MSWaiYvKouYv" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    应变能力
                </td>
                <td class="TD_STYLE2">
                    <select name="MSYingBianNengLi" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    EQ
                </td>
                <td class="TD_STYLE2">
                    <select name="MSEQ" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    IQ
                </td>
                <td class="TD_STYLE2">
                    <select name="MSIQ" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    综合素质
                </td>
                <td class="TD_STYLE1">
                    <select name="MSZongHeSuZhi" class="SELECT_STYLE1">
                        <option value="A">A</option>
                        <option value="B">B</option>
                        <option value="C">C</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    面试人
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="MSMianShiRen" value="zhangsan" class="INPUT_STYLE2" />
                </td>
                <td class="TD_STYLE1">
                    面试时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="MSMianShiTime" onclick="aa('MSMianShiTime')"
                           class="INPUT_STYLE2" id="date">
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    面试评价
                </td>
                <td class="TD_STYLE2" colspan="7">
                    <textarea name="engageInterview.interviewComment" class="TEXTAREA_STYLE1"
                              rows="4"></textarea>
                </td>
            </tr>
        </table>
    }
        
</body>

</html>
