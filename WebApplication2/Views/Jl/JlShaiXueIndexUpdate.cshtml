            methods: {    //函数集
                qr: function () {
                    $.ajax({
                        url: "/Jl/JlShaiXueIndexUpdate1?Id=" + $("#Id").val() + "&JlTuiJianRen=" + $("#JlTuiJianRen").val() + "&JlTuiJianTime=" + $("#JlTuiJianTime").val() + "&sxyj=" + $("#sxyj").val(),
                        type: "post",
                        data: "",
                        success: function (msg) {
                            var dts = eval("(" + msg + ")");
                            if (dts>0) {
                                alert("筛选成功");
                                location.href = '/Jl/JlShaiXueIndexShow?MCName= @Session["MCName"] &MKName=@Session["MKName"]&GuanJianZi=@Session["GuanJianZi"]&JlZcTime= @Session["JlZcTime"] &JlZcOverTime= @Session["JlZcOverTime"]';
                            } else {
                                alert("筛选失败");
                            };
                        }
                    });
                }
            },
