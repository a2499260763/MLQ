    <script type="text/javascript">
        
        var vm = new Vue({
            el: "#sp", //绑定标签
            data: {
                nums: "",
                numss:""//数据传递
            },
            methods: {    //函数集
                load: function () {
                    t = this;//把控制层赋值给t
                    $.ajax({
                        url: "/Jl/JlRegisterIndex2",
                        type: "post",
                        data: "",
                        success: function (msg) {
                            var dts = eval("(" + msg + ")");
                            t.nums = dts;
                        }
                    });
                },
                mkgb: function () {
                    t = this;//把控制层赋值给t
                    $.ajax({
                        url: "/Jl/JlRegisterIndex3",
                        type: "post",
                        data: "MKName=" + $("#MKName").val(),
                        success: function (msg) {
                            var dts = eval("(" + msg + ")");
                            t.numss = dts;
                        }
                    });
                },
                insert: function () {
                    
                    $.ajax({
                        url: "/Jl/JlRegisterInsertIndex",
                        type: "post",
                        data: "MKName=" + $("#MKName").val() + "&MCName=" + $("#MCName").val() + "&ZPName=" + $("#ZPName").val() + "&JlName=" + $("#JlName").val() + "&JlSex=" + $("#JlSex").val() + "&JlEmail=" + $("#JlEmail").val() + "&JlDianHua=" + $("#JlDianHua").val() + "&JlJiaTingDianHua=" + $("#JlJiaTingDianHua").val() + "&JlShouJi=" + $("#JlShouJi").val() + "&JlAdds=" + $("#JlAdds").val() + "&JlYouBian=" + $("#JlYouBian").val() + "&JlGuoJi=" + $("#JlGuoJi").val() + "&JlBreathAdds=" + $("#JlBreathAdds").val() + "&JlBreathDay=" + $("#JlBreathDay").val() + "&JlMingZu=" + $("#JlMingZu").val() + "&JlZongJioaXingYang=" + $("#JlZongJioaXingYang").val() + "&JlZhengZhiMianMao=" + $("#JlZhengZhiMianMao").val() + "&JlEntityCond=" + $("#JlEntityCond").val() + "&JlAge=" + $("#JlAge").val() + "&JlBiYanShooln=" + $("#JlBiYanShooln").val() + "&JlXueli=" + $("#JlXueli").val() + "&JlJiaoYvYean=" + $("#JlJiaoYvYean").val() + "&JlXueLiZhuanYe=" + $("#JlXueLiZhuanYe").val() + "&JlXingZiYaoQiu=" + $("#JlXingZiYaoQiu").val() + "&JlZcTime=" + $("#JlZcTime").val() + "&JlTeChang=" + $("#JlTeChang").val() + "&JlAiHoa=" + $("#JlAiHoa").val() + "&JlGeRenLvLi=" + $("#JlGeRenLvLi").val() + "&JlBeiZhu=" + $("#JlBeiZhu").val(),
                        success: function (msg) {
                            var dts = eval("(" + msg + ")");
                            if (dts > 0) {
                                alert("登记成功");
                                location.href = '/Jl/JlRegisterIndex1'
                            } else {
                                alert("登记失败")
                            }
                        }
                    });
                }
            },
          
            created: function () {   //页面加载方法
                if (@ViewBag.Id>0) {
                    $("#MKName").remove();
                    $("#MCName").remove();
                    $("#kk").html("@ViewBag.kk")
                    $("#cc").html("@ViewBag.cc")
                } else

                this.load();

                }
        });
    </script>
</body>

</html>

